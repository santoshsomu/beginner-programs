#calculator
def add(n1, n2):
  return n1+n2

def subtract(n1, n2):
 return n1-n2

def multiply(n1, n2):
  return n1*n2

def divide(n1, n2):
  return n1/n2

operations={
  "+":add,
  "-":subtract,
  "*":multiply,
  "/": divide 
}

num1 = int(input("what's the first number?\n"))
num2 = int(input("what the second number?\n"))

for symbol in operations:
  print (symbol)

operation_symbol =input("Select an operation from above/n")
calculating_function = operations[operation_symbol]
answer = calculating_function(num1,num2)
print(f"{num1} {operation_symbol} {num2} = {answer}")

print("thank you")
